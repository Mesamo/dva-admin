{
  "entry": "src/index.js",
  "hash": true,
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": true }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": true }]
      ]
    }
  },
  "proxy": {
    "/api": {
      "target": "http://jsonplaceholder.typicode.com/",
      "changeOrigin": true,
      "pathRewrite": { "^/api" : "" }
    }
  },
  "theme": {
      "@icon-url": "'/assets/iconfont/iconfont'"
  },
  "dllPlugin": {
    "exclude": [
      "babel-runtime"
    ],
    "include": [
      "react",
      "react-dom",
      "antd",
      "dva/router",
      "dva/saga",
      "dva/fetch",
      "firebase/app",
      "firebase/auth",
      "firebase/database"
    ]
  }
}
